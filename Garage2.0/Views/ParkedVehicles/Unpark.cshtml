@model LeavingVehicleViewModel

@{
    ViewData["Title"] = "Leave the garage";
}



<h3 class="text-info">Are you sure you want unpark this vehicle?</h3>
<div class="align-self-center">
    <h4 class="text-info">Vehicle details:</h4>
    <hr />
    <dl class="row text-info table table-dark table-striped">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.LeavingVehicle.VehicleType)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.LeavingVehicle.VehicleType)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.LeavingVehicle.RegNr)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.LeavingVehicle.RegNr)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.LeavingVehicle.Color)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.LeavingVehicle.Color)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.LeavingVehicle.Brand)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.LeavingVehicle.Brand)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.LeavingVehicle.Model)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.LeavingVehicle.Model)
        </dd>
        <dt class="col-sm-2">
            Total parking (hours)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ParkingTime)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ParkingCost)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ParkingCost)
        </dd>
    </dl>
    </div>
    @{ 
        Html.RenderPartial("UnparkFooter");
    }
  


  
    

     

